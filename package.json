{
  "name": "pump-web-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "cache-posts": "node lib/cache.js",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx}\"",
    "lint": "next lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && prettier --write sections components pages && npm run cache-posts && git add cache/cache.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint '*/**/*.{js,jsx}' --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^2.30.3",
    "@styled-icons/bootstrap": "^10.34.0",
    "axios": "^0.24.0",
    "emailjs-com": "^3.2.0",
    "gray-matter": "^4.0.3",
    "multiselect-react-dropdown": "^2.0.7",
    "next": "^12.0.4",
    "next-mdx-remote": "^3.0.8",
    "nprogress": "^0.2.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-toastify": "^8.1.0",
    "styled-components": "^5.3.3",
    "swiper": "^7.2.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.13.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.2.6",
    "prisma": "^2.30.3"
  }
}
