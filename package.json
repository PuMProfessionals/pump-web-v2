    "dotenv": "^16.0.3",
    "easymde": "^2.18.0",
    "emailjs-com": "^3.2.0",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^8.5.1",
    "multiselect-react-dropdown": "^2.0.21",
    "next": "^12.3.1",
    "next-mdx-remote": "^4.0.0",
    "node-forge": "^1.3.1",
    "nprogress": "^0.2.0",
    "octokit": "^2.0.10",
