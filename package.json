    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^4.3.8",
    "lint-staged": "^11.2.6",
    "prisma": "^3.10.0"
